generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_POSTGRES_URL")
}

model Comment {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  content   String
  blogId    String
  createdAt DateTime @default(now())
}
